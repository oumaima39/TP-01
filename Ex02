#include <stdio.h >
int main () {
int mat[10] [10]
int i ,j , x;
for(i=0;i<10;i++)
     for(j=0;j<10;j++)
           if(i=j)
       mat[i][j]=-20;
       else  
       if (i>j)
       x=mat[i][j]
       mat [i][j]=mat [j][i]
       for(i=0;i<10;i++)
             for (j=0;j<10;j++)
             printf("%c ,/t",matrix[i][j]);
             printf("\n");
             return 0;
               }
